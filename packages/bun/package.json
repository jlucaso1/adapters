    "@astrojs/test-utils": "workspace:*",
    "@types/bun": "^1.1.9",
    "@types/node": "^18.19.50",
    "@types/server-destroy": "^1.0.4",
    "astro": "^4.15.4",
    "astro-scripts": "workspace:*",
    "cheerio": "1.0.0",
