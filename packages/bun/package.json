  "version": "8.3.3",
  "type": "module",
  "types": "./dist/index.d.ts",
  "author": "withastro",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/withastro/adapters.git",
    "directory": "packages/bun"
  },
  "keywords": ["withastro", "astro-adapter"],
  "bugs": "https://github.com/withastro/adapters/issues",
  "homepage": "https://docs.astro.build/en/guides/integrations-guide/bun/",
